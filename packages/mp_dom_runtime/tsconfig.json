{
  "compilerOptions": {
    "target": "ES5",
    "module": "commonjs",
    "outDir": "./dist",
    "lib": ["ES2015.Promise", "DOM", "ES5"],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noEmitHelpers": true
  },
  "exclude": ["packages"]
}
